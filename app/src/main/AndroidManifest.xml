<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reyzeny.profiledesign">

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme2">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecipientListActivity"/>
        <activity android:name=".ContactUs"/>
        <activity android:name=".MessageSent"/>
        <activity android:name=".RecipientDetailPage"/>
        <activity android:name=".RecipientEditPage"/>
        <activity android:name=".PersonalDetail"/>
        <activity android:name=".ChangePinCode"/>
    </application>

</manifest>